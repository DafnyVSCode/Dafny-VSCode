{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2017",
        "outDir": "../client/server",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "strict": true
    },
    "exclude": [
        "node_modules"
    ]
}
