{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "rootDir": ".",
        "alwaysStrict": true,
        "noImplicitReturns": true,
        "noImplicitThis": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test",
        "test"
    ]
}